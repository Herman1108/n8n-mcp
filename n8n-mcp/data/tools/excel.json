{
  "name": "excel",
  "description": "Read and write Excel files (.xlsx)",
  "input_schema": {
    "type": "object",
    "properties": {
      "file_path": {
        "type": "string"
      },
      "operation": {
        "type": "string",
        "enum": [
          "read",
          "write"
        ]
      }
    },
    "required": [
      "file_path",
      "operation"
    ]
  }
}